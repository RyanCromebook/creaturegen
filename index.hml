var user = null;
function myName(){
  Prompt.render('Enter Your Full Name','setName');
}
function setName(val){
  user = val;
}
function addQuote(){
  Prompt.render('What do you want your abjective to be?', 'getQuote');
}
function getQuote(quote){
  if(quote === " " || quote === "" || quote === null || quote === undefined){
    addQuote();
  } else {
    quotes.push(`"${quote}"<br><i style="font-size:50%">~${user}</i>`);
    max = eval(max + 1);
  }
}
var prev;
function newQuote(){
  let ans = Math.floor(Math.random() * max) + 0;
  let res = document.getElementsByClassName('quote')[0];
  res.innerHTML = quotes[ans];
};
